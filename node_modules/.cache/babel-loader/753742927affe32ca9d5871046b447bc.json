{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\nimport axios from 'axios';\nexport default {\n  name: 'ArtworkDetail',\n\n  data() {\n    return {\n      film: {},\n      items: []\n    };\n  },\n\n  created() {\n    let filmID = this.$route.params.id;\n    console.log(filmID);\n    this.getFilm(filmID);\n    this.generateTable();\n  },\n\n  methods: {\n    getFilm(filmId) {\n      axios.get(`https://ghibliapi.herokuapp.com/films/${filmId}`).then(resp => {\n        this.film = resp.data;\n      });\n    },\n\n    generateTable() {\n      this.items = [{\n        name: 'Director',\n        value: this.film.director\n      }, {\n        name: 'Producer',\n        value: this.film.producer\n      }, {\n        name: 'Release Date',\n        value: this.film.release_date\n      }, {\n        name: 'Rotten Tomatoes',\n        value: this.film.rt_score\n      }];\n    }\n\n  }\n};","map":{"version":3,"sources":["ArtworkDetail.vue"],"names":[],"mappings":";;;;;;;;;AAUA,OAAA,KAAA,MAAA,OAAA;AAEA,eAAA;EACA,IAAA,EAAA,eADA;;EAEA,IAAA,GAAA;IACA,OAAA;MACA,IAAA,EAAA,EADA;MAEA,KAAA,EAAA;IAFA,CAAA;EAIA,CAPA;;EAQA,OAAA,GAAA;IACA,IAAA,MAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA;IAEA,OAAA,CAAA,GAAA,CAAA,MAAA;IACA,KAAA,OAAA,CAAA,MAAA;IACA,KAAA,aAAA;EACA,CAdA;;EAeA,OAAA,EAAA;IACA,OAAA,CAAA,MAAA,EAAA;MACA,KAAA,CAAA,GAAA,CAAA,yCAAA,MAAA,EAAA,EAAA,IAAA,CAAA,IAAA,IAAA;QACA,KAAA,IAAA,GAAA,IAAA,CAAA,IAAA;MACA,CAFA;IAGA,CALA;;IAMA,aAAA,GAAA;MACA,KAAA,KAAA,GAAA,CACA;QACA,IAAA,EAAA,UADA;QAEA,KAAA,EAAA,KAAA,IAAA,CAAA;MAFA,CADA,EAKA;QACA,IAAA,EAAA,UADA;QAEA,KAAA,EAAA,KAAA,IAAA,CAAA;MAFA,CALA,EASA;QACA,IAAA,EAAA,cADA;QAEA,KAAA,EAAA,KAAA,IAAA,CAAA;MAFA,CATA,EAaA;QACA,IAAA,EAAA,iBADA;QAEA,KAAA,EAAA,KAAA,IAAA,CAAA;MAFA,CAbA,CAAA;IAkBA;;EAzBA;AAfA,CAAA","sourcesContent":["<template>\n  <div class=\"artwork-detials-wrapper\">\n    <h2 class=\"film-title\">{{ film.title }}</h2>\n    <img class=\"film-image\" :src=\"film.image\" alt=\"\">\n    <p class=\"description\">{{ film.description }}</p>\n    <b-table striped hover :items=\"items\"></b-table>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n    name: 'ArtworkDetail',\n    data() {\n        return {\n            film: {},\n            items: [],\n        }\n    },\n    created() {\n        let filmID = this.$route.params.id;\n\n        console.log(filmID);\n        this.getFilm(filmID);\n        this.generateTable();\n    },\n    methods: {\n        getFilm(filmId) {\n            axios.get(`https://ghibliapi.herokuapp.com/films/${filmId}`).then(resp => {\n                this.film = resp.data;\n            });\n        },\n        generateTable() {\n            this.items = [\n                {\n                    name: 'Director',\n                    value: this.film.director,\n                },\n                {\n                    name: 'Producer',\n                    value: this.film.producer,\n                },\n                {\n                    name: 'Release Date',\n                    value: this.film.release_date,\n                },\n                {\n                    name: 'Rotten Tomatoes',\n                    value: this.film.rt_score,\n                },\n            ];\n        },\n    }\n}\n</script>\n\n<style>\n.artwork-detials-wrapper {\n}\n\n.film-title {\n  color: white;\n}\n\n.film-image {\n\n}\n\n.description {\n    color: white;\n    font-size: 21px;\n    margin: 2rem 8rem;\n    text-align: left;\n}\n\n</style>"],"sourceRoot":"src/components"},"metadata":{},"sourceType":"module"}