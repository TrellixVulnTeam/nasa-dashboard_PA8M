: '',\n    }\n    },\n    created() {\n        this.GetSpecies();\n    },\n    computed: {\n        filteredSpecies() {\n            return this.species.filter(type => {\n                return type.name.toLowerCase().indexOf(this.search.toLowerCase()) > -1\n            })\n        }\n    },\n    methods: {\n    GetSpecies() {\n        axios.get('https://ghibliapi.herokuapp.com/species').then(resp => {\n            this.species = resp.data;\n        });\n    },\n    }\n\n}\n</script>\n\n<style scoped lang=\"scss\">\n\n    .container {\n        max-width: 600px;\n        margin: 50px auto;\n    }\n\n h1 {\n    padding: 20px;\n    color: white;\n  }\n\n  .no-list {\n    list-style: none;\n  }\n\n  .person-name {\n    color: white;\n  }\n  .no-styling {\n    text-decoration: none;\n    color: black;\n    font-weight: 600;\n  }\n  .search-field {\n    max-width: 600px;\n    margin: auto;\n  }\n\n</style>"],"sourceRoot":"src/components"},"metadata":{},"sourceType":"module"}